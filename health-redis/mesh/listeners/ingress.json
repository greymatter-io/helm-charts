{
    "zone_key": "zone-default-zone",
    "listener_key": "listener-health-redis-ingress",
    "domain_keys": [
        "domain-health-redis-ingress"
    ],
    "name": "ingress",
    "ip": "0.0.0.0",
    "port": 10808,
    "protocol": "http_auto",
    "secret": {
        "secret_key": "health-redis.identity",
        "secret_name": "spiffe://quickstart.greymatter.io/health-redis",
        "secret_validation_name": "spiffe://quickstart.greymatter.io",
        "ecdh_curves": [
            "X25519:P-256:P-521:P-384"
        ]
    },
    "tracing_config": null,
    "active_network_filters": ["envoy.rbac", "envoy.tcp_proxy"],
    "network_filters": {
        "envoy_rbac": {
            "stat_prefix": "ingress",
            "rules": {
              "action": 0,
              "policies": {
                "0-proxies": {
                  "permissions": [
                    {
                      "any": true
                    }
                  ],
                  "principals": [
                    {
                      "authenticated": {
                        "principal_name": {
                            "safe_regex": {
                                "google_re2": {},
                                "regex": "spiffe:\/\/quickstart.greymatter.io\/(.+)"
                            }
                        }
                      }
                    }
                  ]
                }
              }
            }
          },
        "envoy_tcp_proxy": {
            "stat_prefix": "ingress_redis",
            "cluster": "health-redis-local"
          }
    }
}
