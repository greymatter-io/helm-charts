{
  "zone_key": "{{ .Values.gmControlApi.zone }}",
  "proxy_key": "edge-oauth-proxy",
  "domain_keys": ["edge-oauth"],
  "listener_keys": ["edge-oauth-listener"],
  "name": "edge-oauth",
  "listeners": null,
  "active_proxy_filters": ["gm.metrics", "gm.oauth", "gm.inheaders"],
  "proxy_filters": {
    "gm_oauth": {
      "provider": "{{ .Values.global.edgeOAuth.provider }}",
      "client_id": "{{ .Values.global.edgeOAuth.client_id }}",
      "client_secret": "{{ .Values.global.edgeOAuth.client_secret }}",
      "server_name": "{{ .Values.global.edgeOAuth.server_name }}",
      "session_secret": "{{ .Values.global.edgeOAuth.server_secret }}",
      "domain": ".*"
    },
    "gm_metrics": {
      "metrics_port": 8081,
      "metrics_host": "0.0.0.0",
      "metrics_dashboard_uri_path": "/metrics",
      "metrics_prometheus_uri_path": "/prometheus",
      "metrics_ring_buffer_size": 4096,
      "prometheus_system_metrics_interval_seconds": 15,
      "metrics_key_function": "none"
    }
  }
}
